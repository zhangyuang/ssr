{
  "name": "ssr-core",
  "version": "0.1.1",
  "description": "core for serverless ssr",
  "main": "./cjs/index.js",
  "module": "./esm/index.js",
  "sideEffects": false,
  "scripts": {
    "build": "concurrently \"tsc -p ./tsconfig.cjs.json --sourceMap false\" \" tsc -p ./tsconfig.esm.json --sourceMap false\"",
    "watch": "concurrently \"tsc -w -p ./tsconfig.cjs.json \" \"tsc -w -p ./tsconfig.esm.json \"",
    "watch:cjs": "tsc -w -p ./tsconfig.cjs.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ykfe/ssr.git"
  },
  "keywords": [
    "cli",
    "serverless",
    "ssr",
    "react"
  ],
  "author": "zhangyuang",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ykfe/ssr/issues"
  },
  "homepage": "https://github.com/ykfe/ssr#readme",
  "devDependencies": {
    "@types/koa": "^2.11.3",
    "@types/koa-router": "^7.4.0",
    "@types/mocha": "^7.0.2",
    "@types/webpack": "^4.41.8",
    "@types/webpack-dev-server": "^3.10.1",
    "ssr-types": "^0.1.1"
  },
  "dependencies": {
    "@midwayjs/serverless-invoke": "^0.2.67",
    "koa": "^2.11.0",
    "koa-proxy": "^1.0.0-alpha.3",
    "koa-router": "^8.0.8",
    "koa-static": "^5.0.0",
    "webpack": "^4.42.1",
    "webpack-dev-server": "^3.10.3"
  }
}
