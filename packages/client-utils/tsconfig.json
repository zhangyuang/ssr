{
  "compileOnSave": true,
  "compilerOptions": {
   
    "moduleResolution": "node",
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "inlineSourceMap":true,
    "noImplicitThis": true,
    "noUnusedLocals": true,
    "stripInternal": true,
    "pretty": true,
    "declaration": true,
    "strict": true,
    "jsx": "react",
    "outDir": "./output"
  },
  "include": [
      "src"
  ]
}