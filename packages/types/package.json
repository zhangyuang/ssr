{
  "name": "ssr-types",
  "version": "6.2.24",
  "description": "types for serverless ssr",
  "main": "./cjs/index.js",
  "module": "./esm/index.js",
  "scripts": {
    "build": "concurrently \"tsc -p ./tsconfig.cjs.json \" \" tsc -p ./tsconfig.esm.json\"",
    "build:publish": "concurrently \"tsc -p ./tsconfig.cjs.json --sourcemap false\" \" tsc -p ./tsconfig.esm.json --sourcemap false\"",
    "watch": "concurrently \"tsc -w -p ./tsconfig.cjs.json \" \"tsc -w -p ./tsconfig.esm.json \""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zhangyuang/ssr.git"
  },
  "keywords": [
    "cli",
    "serverless",
    "ssr",
    "react",
    "vue"
  ],
  "author": "zhangyuang",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/zhangyuang/ssr/issues"
  },
  "homepage": "https://github.com/zhangyuang/ssr#readme",
  "peerDependencies": {
    "@types/node": ">=14.0.0"
  },
  "devDependencies": {
    "concurrently": "^5.1.0",
    "@babel/core": "^7.0.0",
    "@midwayjs/decorator": "^3.0.0",
    "@midwayjs/koa": "^3.0.0",
    "@rollup/plugin-babel": "^5.3.0",
    "@types/babel__core": "^7.1.18",
    "@types/cookies": "^0.7.7",
    "@types/estree": "^0.0.51",
    "@types/express": "^4.17.11",
    "@types/koa-router": "^7.4.4",
    "@types/tapable": "^1.0.6",
    "@types/webpack": "^4.41.10",
    "@types/yargs": "^13.0.4",
    "vite": "^2.9.13",
    "webpack-chain": "^6.4.0",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-router-dom": "^5.1.6"
  }
}
