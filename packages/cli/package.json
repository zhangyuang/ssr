{
  "name": "ssr",
  "version": "0.2.4",
  "description": "cli for serverless ssr",
  "main": "./cjs/index.js",
  "module": "./esm/index.js",
  "sideEffects": false,
  "scripts": {
    "build": "concurrently \"tsc -p ./tsconfig.cjs.json --inlineSourceMap false\" \" tsc -p ./tsconfig.esm.json --inlineSourceMap false\"",
    "watch": "concurrently \"tsc -w -p ./tsconfig.cjs.json \" \"tsc -w -p ./tsconfig.esm.json \"",
    "watch:cjs": "tsc -w -p ./tsconfig.cjs.json"
  },
  "bin": {
    "ssr": "./cjs/cli.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ykfe/ssr.git"
  },
  "keywords": [
    "cli",
    "serverless",
    "ssr",
    "react"
  ],
  "author": "zhangyuang",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ykfe/ssr/issues"
  },
  "homepage": "https://github.com/ykfe/ssr#readme",
  "dependencies": {
    "@midwayjs/faas-cli": "^0.2.71",
    "dclone": "^1.2.0",
    "inquirer": "^7.1.0",
    "shelljs": "^0.8.3",
    "ssr-server-utils": "^0.2.4",
    "ssr-webpack": "^0.2.4",
    "yargs": "^13.2.4"
  },
  "devDependencies": {
    "@types/inquirer": "^6.5.0",
    "@types/shelljs": "^0.8.7",
    "@types/yargs": "^13.0.0",
    "concurrently": "^5.1.0",
    "ssr-types": "^0.2.4"
  }
}
