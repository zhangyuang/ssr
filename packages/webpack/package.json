{
  "name": "ssr-webpack",
  "version": "0.2.27",
  "description": "webpack config for serverless ssr",
  "main": "./cjs/index.js",
  "module": "./esm/index.js",
  "sideEffects": false,
  "scripts": {
    "build": "concurrently \"tsc -p ./tsconfig.cjs.json --inlineSourceMap false\" \" tsc -p ./tsconfig.esm.json --inlineSourceMap false\"",
    "watch": "concurrently \"tsc -w -p ./tsconfig.cjs.json \" \"tsc -w -p ./tsconfig.esm.json \"",
    "watch:cjs": "tsc -w -p ./tsconfig.cjs.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ykfe/ssr.git"
  },
  "keywords": [
    "webpack",
    "serverless",
    "ssr",
    "react"
  ],
  "author": "zhangyuang",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ykfe/ssr/issues"
  },
  "homepage": "https://github.com/ykfe/ssr#readme",
  "dependencies": {
    "@babel/core": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.9.6",
    "@babel/preset-env": "^7.5.5",
    "@midwayjs/faas-dev-pack": "*",
    "babel-loader": "8.0.4",
    "babel-plugin-import": "^1.13.0",
    "babel-preset-react-app": "^9.0.2",
    "concurrently": "^5.1.0",
    "core-js": "^3.6.4",
    "css-hot-loader": "^1.4.4",
    "css-loader": "1.0.0",
    "css-modules-require-hook": "^4.2.3",
    "file-loader": "2.0.0",
    "ignore-loader": "^0.1.2",
    "koa": "^2.11.0",
    "koa-proxy": "^1.0.0-alpha.3",
    "less": "^3.9.0",
    "less-loader": "^4.1.0",
    "mini-css-extract-plugin": "^0.9.0",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "postcss-flexbugs-fixes": "4.1.0",
    "postcss-loader": "3.0.0",
    "postcss-preset-env": "^6.0.5",
    "postcss-safe-parser": "4.0.1",
    "react": "^16.13.1",
    "react-dev-utils": "^10.2.1",
    "react-dom": "^16.13.1",
    "react-router-dom": "^5.1.2",
    "ssr-client-utils": "^0.2.25",
    "ssr-server-utils": "^0.2.26",
    "terser-webpack-plugin": "^2.3.5",
    "url-loader": "1.1.1",
    "webpack": "^4.42.1",
    "webpack-bundle-analyzer": "^3.6.1",
    "webpack-chain": "^6.4.0",
    "webpack-dev-server-ssr": "^3.10.3",
    "webpack-manifest-plugin": "^2.2.0"
  },
  "devDependencies": {
    "@types/koa": "^2.11.3",
    "@types/react": "^16.9.25",
    "@types/react-dom": "^16.9.5",
    "@types/react-router-dom": "^5.1.3",
    "@types/webpack": "^4.41.8",
    "@types/webpack-dev-server": "^3.10.1",
    "postcss-discard-comments": "^4.0.2"
  }
}
